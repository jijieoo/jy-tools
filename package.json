{
  "name": "@jy-workspace/tools",
  "publishConfig": {
    "access": "public"
  },
  "description": "一些函数",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "module": "./dist/jy-tools.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/jy-tools.js"
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf dist && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "prettier . --write --cache",
    "prepare": "husky",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "author": "jiyuchen",
  "repository": {
    "type": "git",
    "url": "https://github.com/jijieoo/jy-tools.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/jijieoo/jy-tools#readme"
}
